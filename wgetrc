# Change default UA to Win10+FF
# Override with `--user-agent="string"`.
user-agent = "Mozilla/5.0 (Windows NT 10.0; WOW64; rv:75.0.0) Gecko/20100101 Firefox/75.0"

# Turn timestamping on
timestamping = on

# Set timeout a bit shorter, default is 900s
timeout = 60

# Print HTTP server response
server_response = on

# We are not robots <[*_*]>
robots = off
